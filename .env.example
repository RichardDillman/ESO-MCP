# ESO Logs API Credentials
# Get these from: https://www.esologs.com/api/clients/
ESOLOGS_CLIENT_ID=your_client_id_here
ESOLOGS_CLIENT_SECRET=your_client_secret_here

# Telegram Bridge (Optional - for InnerVoice notifications)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Google Chat Webhook (Optional)
GOOGLE_CHAT_WEBHOOK_URL=your_webhook_url

# Slack Webhook (Optional)
SLACK_WEBHOOK_URL=your_webhook_url

# ntfy.sh Push Notifications (Optional)
NTFY_TOPIC=your_ntfy_topic
NTFY_URL=https://ntfy.sh

# SMS via Email Gateway (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
SMTP_FROM=your_email@gmail.com
SMS_GATEWAY_ADDRESS=1234567890@txt.att.net

# Vercel Deployment (Optional)
VERCEL_TOKEN=your_vercel_token

# Database (SQLite by default, no config needed)
# DATABASE_URL="file:./prisma/dev.db"
