{
  "name": "eso-mcp-server",
  "version": "0.1.0",
  "description": "MCP server for Elder Scrolls Online data from UESP wiki",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "prisma generate && tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "update-data": "tsx scripts/update-data.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate"
  },
  "keywords": [
    "mcp",
    "elder-scrolls-online",
    "eso",
    "uesp",
    "game-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@prisma/adapter-libsql": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "dotenv": "^17.2.3",
    "nodemailer": "^7.0.10",
    "prisma": "^7.0.0",
    "sharp": "^0.34.5",
    "tesseract.js": "^6.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
